{
	"name": "tanstack-db-atom",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/harrytran998/tanstack-db-atom.git"
	},
	"bugs": {
		"url": "https://github.com/harrytran998/tanstack-db-atom/issues"
	},
	"homepage": "https://github.com/harrytran998/tanstack-db-atom#readme",
	"description": "TanStack DB utilities for Effect Atom - provides reactive atoms that integrate with TanStack DB collections and queries",
	"keywords": [
		"tanstack",
		"db",
		"effect",
		"atom",
		"reactive",
		"typescript",
		"database",
		"query"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"package.json"
	],
	"scripts": {
		"build": "bun run build:types && bun run build:bundle",
		"build:bundle": "rolldown -c",
		"build:types": "tsc -p tsconfig.build.json",
		"dev": "rolldown -c --watch",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"@effect-atom/atom-react": ">=0.4.0",
		"@tanstack/db": ">=0.5.0",
		"effect": ">=3.0.0"
	},
	"dependencies": {
		"@effect-atom/atom-react": "^0.4.4",
		"@tanstack/db": "0.5.18",
		"effect": "3.19.14"
	},
	"devDependencies": {
		"@effect/language-service": "0.64.1",
		"rolldown": "^1.0.0-beta.1",
		"typescript": "^5.9.3"
	}
}
